{
  "name": "nosql-db-fullstack",
  "version": "1.0.0",
  "description": "NoSQL Database with Go backend and Vue.js frontend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd . && ./nosql-server -config config/collections.json",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["nosql", "database", "go", "vue", "vite"],
  "author": "",
  "license": "MIT"
} 